{
    "name": "reach-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server -d --config webpack.config.js --mode development",
        "build": "webpack --config webpack.prod.config.js --mode production",
        "test": "jest",
        "predeploy": "npm run build",
        "deploy": "gh-pages-d-build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fiona-wong/reach-project.git"
    },
    "author": "fiona",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/fiona-wong/reach-project/issues"
    },
    "homepage": "https://fiona-wong.github.io/word-guessing-game/",
    "jest": {
        "setupFilesAfterEnv": [
            "./enzyme.setup.js"
        ],
        "transform": {
            "^.+\\.js?$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
        }
    },
    "devDependencies": {
        "@babel/cli": "^7.1.0",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.7.1",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^2.0.1",
        "css-loader": "^1.0.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.12.1",
        "eslint": "^5.16.0",
        "eslint-config-react": "^1.1.7",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-react": "^7.12.4",
        "file-loader": "^3.0.1",
        "gh-pages": "^2.0.1",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^4.6.0",
        "jest": "^24.7.1",
        "style-loader": "^0.23.0",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.19.1",
        "webpack-cli": "^3.1.1",
        "webpack-dev-server": "^3.3.1"
    },
    "dependencies": {
        "bootstrap": "^4.3.1",
        "cors": "^2.8.5",
        "react": "^16.5.2",
        "react-bootstrap": "^1.0.0-beta.8",
        "react-dom": "^16.5.2",
        "react-hot-loader": "^4.3.11",
        "request": "^2.88.0",
        "sass": "^1.18.0"
    }
}
